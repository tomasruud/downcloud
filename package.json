{
  "name": "downcloud",
  "version": "4.0.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build:css": "postcss src/index.css -o src/index.dist.css",
    "watch:css": "postcss src/index.css -o src/index.dist.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "NODE_ENV=production npm run build:css && react-scripts build",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --write"
  },
  "engines": {
    "node": "^14"
  },
  "dependencies": {
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/webpack-env": "^1.15.3",
    "autoprefixer": "^10.0.2",
    "axios": "^0.21.0",
    "multi-download": "^3.0.0",
    "postcss": "^8.1.8",
    "postcss-cli": "^8.3.0",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.2.1",
    "react-scripts": "^4.0.0",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
