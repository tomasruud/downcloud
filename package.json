{
  "version": "2.0.0",
  "private": true,
  "homepage": "https://downcloud.ruud.ninja",
  "scripts": {
    "format": "prettier --write src/*",
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "npm run format && react-scripts test --env=jsdom"
  },
  "dependencies": {
    "multi-download": "^2.0.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "soundcloud": "^3.3.0",
    "spinkit": "^1.2.5",
    "typography": "^0.16.17",
    "typography-theme-noriega": "^0.15.10"
  },
  "devDependencies": {
    "prettier": "^1.12.1",
    "react-snapshot": "^1.3.0"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "parser": "babylon",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "jsxBracketSameLine": true
  }
}
